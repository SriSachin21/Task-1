package Practice;

public class Exe6 {

	public static void main(String[] args) {
		int k=5;
		
		for(int i=0;i<5;i++) {
			int num=k;
			for(int j=0;j<=i;j++) {
				System.out.print(num+ " ");
				num--;
			}
			for(int j=i+1;j<k;j++) {
				System.out.print(k-1+" ");
			}
			System.out.println();
		}

	}

}
